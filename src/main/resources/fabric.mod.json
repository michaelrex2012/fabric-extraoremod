{
	"schemaVersion": 1,
	"id": "eom",
	"version": "${version}",
	"name": "Extra Ore Mod",
	"description": "Mod that add more utilities and ores to Minecraft. Still in early development.",
	"authors": [
		"@michaelrex2012"
	],
	"contact": {
		"homepage": "https://modrinth.com/mod/extra-ore-mod",
		"sources": "https://github.com/michaelrex2012/fabric-extraoremod"
	},
	"license": "MIT",
	"icon": "assets/eom/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.michael.eom.TutorialMod"
		],
		"client": [
			"net.michael.eom.TutorialModClient"
		],
		"fabric-datagen": [
			"net.michael.eom.TutorialModDataGenerator"
		]
	},
	"mixins": [
		"tutorialmod.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.21",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}